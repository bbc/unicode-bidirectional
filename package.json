{
  "name": "unicode-bidirectional",
  "version": "2.0.5",
  "description": "An implementation of Unicode 9.0.0 Bidirectional Algorithm",
  "main": "dist/unicode.bidirectional.js",
  "module": "src/main.js",
  "es6": "src/main.js",
  "scripts": {
    "lint": "eslint src/ test/ -c config/eslint.config.json",
    "test": "npm run unit",
    "coverage": "nyc --reporter=lcov npm test",
    "unit": "mocha --compilers js:babel-register -r test/unit/spec.helper.js 'test/unit/**/*.spec.js'",
    "conform": "./test/conform/download_tests.sh && npm run conform-bidiclass && npm run conform-codepoint",
    "conform-bidiclass": "babel-node ./test/conform/bidiclass/runner.js",
    "conform-codepoint": "babel-node ./test/conform/codepoint/runner.js",
    "dist": "webpack --config config/webpack.config.js",
    "dist-min": "WEBPACK_ENV=minify webpack --config config/webpack.config.js"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bbc/unicode-bidirectional.git"
  },
  "author": "BBC",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bbc/unicode-bidirectional/issues"
  },
  "homepage": "https://github.com/bbc/unicode-bidirectional#readme",
  "dependencies": {
    "immutable": "^3.8.1",
    "lodash.flow": "^3.5.0",
    "lodash.identity": "^3.0.0",
    "lodash.includes": "^4.3.0",
    "lodash.isnumber": "^3.0.3",
    "lodash.isundefined": "^3.0.1",
    "unicode-9.0.0": "^0.7.0",
    "unicode-bidiclass": "v1.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.9.0",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^3.5.0",
    "chai-immutable": "^1.6.0",
    "eslint": "^4.1.1",
    "mocha": "^3.4.2",
    "nyc": "^10.1.2",
    "punycode": "^2.1.0",
    "webpack": "^3.0.0"
  }
}
